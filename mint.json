{
  "$schema": "https://mintlify.com/schema.json",
  "name": "celltek",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#0177fb",
    "light": "#0177fb",
    "dark": "#1269cc",
    "anchors": {
      "from": "#0177fb",
      "to": "#1269cc"
    }
  },
  "theme": "prism",
  "modeToggle": {
    "default": "light",
    "isHidden": true
  },
  "topbarCtaButton": {
    "name": "Dashboard",
    "url": "https://celltek.space/customer"
  },
  "tabs": [
    {
      "name": "REST-API",
      "url": "rest"
    },
    {
      "name": "SDKs",
      "url": "sdks"
    },
    {
      "name": "Troubleshooting",
      "url": "troubleshooting"
    }
  ],
  "navigation": [
    {
      "group": "Introduction",
      "pages": [
        "overview",
        "quickstart",
        "development"
      ]
    },
    {
      "group": "Essentials",
      "pages": [
        "essentials/markdown",
        "essentials/code",
        "essentials/images",
        "essentials/settings",
        "essentials/navigation",
        "essentials/reusable-snippets"
      ]
    },
    {
      "group": "API Documentation",
      "pages": [
        "rest/introduction"
      ]
    },
    {
      "group": "Endpoint Examples",
      "pages": [
        "api-reference/endpoint/get",
        "api-reference/endpoint/create",
        "api-reference/endpoint/delete"
      ]
    }
  ],
  "footerSocials": {
    "x": "https://celltek.de/x",
    "github": "https://celltek.de/github",
    "discord": "https://celltek.de/discord"
  }
}